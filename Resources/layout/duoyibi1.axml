<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="39.0dp"
        android:id="@+id/gridLayout1"
        android:background="#bad995">
        <Button
            android:text="返回"
            android:textColor="#636363"
            android:id="@+id/button1"
            android:layout_width="36.0dp"
            android:layout_height="36.0dp"
            android:layout_marginRight="0.0dp"
            android:layout_marginBottom="0.0dp"
            android:background="#bad995" />
        <TextView
            android:textColor="#636363"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="100.0dp"
            android:layout_height="36.0dp"
            android:layout_marginLeft="100.0dp"
            android:layout_marginTop="2.0dp"
            android:id="@+id/fixed1"
            android:background="#bad995"
            android:text="剁一笔" />
        <SearchView
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/searchView1"
            android:layout_marginLeft="100dp"
            android:background="#b5e7bd"
            android:fitsSystemWindows="true"
            android:backgroundTint="#bad995" />
    </GridLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:id="@+id/linearLayout1"
        android:layout_marginBottom="0.0dp"
        android:background="#ffffffff">
        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:id="@+id/gridLayout2"
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="#bad995">
            <EditText
                android:id="@+id/costinput"
                android:imeOptions="actionSearch"
                android:layout_width="50dp"
                android:layout_height="40.0dp"
                android:hint="0.00"
                android:layout_marginLeft="250px"
                android:layout_marginTop="40px"
                android:layout_marginBottom="20.0dp"
                android:inputType="number" />
        </GridLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="42.0dp"
        android:id="@+id/linearLayout1"
        android:layout_marginBottom="0.0dp"
        android:background="#ffffffff"
        android:minWidth="25px"
        android:minHeight="25px">
        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:id="@+id/gridLayout2"
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="#fffbfbff">
            <TextView
                android:textColor="#636363"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="200dp"
                android:layout_height="40.0dp"
                android:id="@+id/zhouzhichu"
                android:layout_marginLeft="0px"
                android:layout_marginBottom="0.0dp"
                android:gravity="center"
                android:text=" 收入支出类别"
                android:autoText="true"
                android:background="#bad995" />
            <RadioGroup
                android:minWidth="200dp"
                android:minHeight="40.0dp"
                android:id="@+id/radioGroup1"
                android:orientation="horizontal">
                <RadioButton
                    android:layout_width="50dp"
                    android:layout_height="40.0"
                    android:checked="true"
                    android:text="收入"
                    android:id="@+id/shouruButton"
                    android:layout_marginLeft="0dp"
                    android:background="#ffffffff" />
                <RadioButton
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="50px"
                    android:checked="true"
                    android:text="支出"
                    android:id="@+id/zhichuButton" />
            </RadioGroup>
        </GridLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="42.0dp"
        android:id="@+id/linearLayout1"
        android:layout_marginBottom="0.0dp"
        android:background="#ffffffff"
        android:minWidth="25px"
        android:minHeight="25px">
        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:id="@+id/gridLayout2"
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="#fffbfbff">
            <TextView
                android:textColor="#636363"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="200dp"
                android:layout_height="40.0dp"
                android:id="@+id/zhouzhichu"
                android:layout_marginLeft="0px"
                android:layout_marginBottom="0.0dp"
                android:gravity="center"
                android:text=" 类别"
                android:autoText="true"
                android:background="#bad995" />
            <RadioGroup
                android:minWidth="match_parent"
                android:minHeight="42.0dp"
                android:id="@+id/radioGroup2"
                android:orientation="horizontal">
                <RadioButton
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="0dp"
                    android:checked="true"
                    android:text="餐饮类"
                    android:id="@+id/canyinButton"
                    android:textColor="#ff0c0707" />
                <RadioButton
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:checked="true"
                    android:text="出行类"
                    android:id="@+id/chuxingButton"
                    android:textColor="#ff170404" />
                <RadioButton
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:checked="true"
                    android:text="医疗类"
                    android:textColor="#ff170404"
                    android:id="@+id/yiliaoButton" />
                <RadioButton
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="18dp"
                    android:checked="true"
                    android:text="生活用品"
                    android:textColor="#ff170404"
                    android:id="@+id/shenghuoButton" />
            </RadioGroup>
        </GridLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="55.0dp"
        android:id="@+id/linearLayout1"
        android:layout_marginBottom="0.0dp"
        android:background="#bad995"
        android:minWidth="25px"
        android:minHeight="25px">
        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:id="@+id/gridLayout2"
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="#bad995">
            <TextView
                android:textColor="#636363"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="40dp"
                android:layout_height="40.0dp"
                android:id="@+id/timeinput"
                android:layout_marginLeft="30px"
                android:layout_marginBottom="0.0dp"
                android:layout_marginTop="15px"
                android:gravity="center"
                android:text="11月"
                android:autoText="true"
                android:background="#bad995" />
            <TextView
                android:textColor="#636363"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="100dp"
                android:layout_height="40.0dp"
                android:id="@+id/zhouzhichu"
                android:layout_marginLeft="0px"
                android:layout_marginBottom="0.0dp"
                android:layout_marginTop="15px"
                android:gravity="center"
                android:text="支付宝"
                android:autoText="true"
                android:background="#bad995" />
            <Button
                android:text="提交"
                android:id="@+id/submit"
                android:layout_marginLeft="230px"
                android:layout_marginTop="10px" />
        </GridLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:id="@+id/linearLayout1"
        android:layout_marginBottom="0.0dp"
        android:background="#ffffffff">
        <Keyboard
            android:keyHeight="50px"
            android:horizontalGap="2px"
            android:verticalGap="2px">
            <Row
                android:keyWidth="32px">
                <Key
                    android:keyLabel="A" />
            </Row>
        </Keyboard>
    </LinearLayout>
</LinearLayout>