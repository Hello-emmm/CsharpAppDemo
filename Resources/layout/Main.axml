                android:text="旅游类"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_height="40.0dp"
                android:layout_width="120.0dp"
                android:layout_marginLeft="10.0dp"
                android:layout_marginTop="16.0dp"
                android:id="@+id/textView8"
                android:layout_marginRight="45.0dp"
                android:textColor="#000000" />
            <TextView
                android:text="0.00"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_height="40.0dp"
                android:layout_width="80.0dp"
                android:layout_marginLeft="10.0dp"
                android:layout_marginTop="16.0dp"
                android:id="@+id/lvyouzhichu"
                android:textColor="#000000"
                android:layout_marginRight="0.0dp" />
            <ImageView
                android:src="@android:drawable/ic_menu_gallery"
                android:layout_width="30.0dp"
                android:layout_height="30.0dp"
                android:layout_marginLeft="16.0dp"
                android:layout_marginTop="16.0dp"
                android:id="@+id/star" />
        </GridLayout>
        <GridLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="62.0dp"
