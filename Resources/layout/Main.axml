<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="39.0dp"
        android:id="@+id/gridLayout1"
        android:background="#EE7600">
        <Button
            android:text="返回"
            android:id="@+id/button1"
            android:layout_width="36.0dp"
            android:layout_height="36.0dp"
            android:layout_marginRight="0.0dp"
            android:layout_marginBottom="0.0dp"
            android:background="#EE7600" />
        <TextView
            android:text="分类统计"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="100.0dp"
            android:layout_height="36.0dp"
            android:layout_marginLeft="100.0dp"
            android:layout_marginTop="2.0dp"
            android:id="@+id/textView16" />
        <SearchView
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/searchView1"
            android:layout_marginLeft="100dp" />
    </GridLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="101.0dp"
        android:id="@+id/linearLayout1"
        android:layout_marginBottom="0.0dp"
        android:background="#EE7600">
        <TextView
            android:text="666.77"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/zongzhichu"
            android:layout_marginLeft="70px" />
        <TextView
            android:text="总支出"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            android:layout_marginLeft="70px" />
        <GridLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="26.5dp"
            android:layout_marginBottom="0.0dp">
            <TextView
                android:text="本月支出"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="76dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView4"
                android:layout_marginLeft="70px"
                android:layout_marginRight="0.0dp" />
            <TextView
                android:text="(本月支出数值)"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width=""
                android:layout_height="wrap_content"
                android:id="@+id/benyuezhichu"
                android:layout_marginLeft="70px"
                android:layout_marginRight="0.0dp" />
        </GridLayout>
        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="26.5dp"
            android:id="@+id/gridLayout7">
            <TextView
                android:text="本月余额"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="76dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView5"
                android:layout_marginLeft="70px" />
            <TextView
                android:text="（本月余额数值）"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width=""
                android:layout_height="wrap_content"
                android:id="@+id/benyueyue"
                android:layout_marginLeft="70px" />
        </GridLayout>
    </LinearLayout>
    <GridLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="35.5dp"
        android:id="@+id/linearLayout2"
        android:background="#FFF">
        <TextView
            android:text="选择月份"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="175.0dp"
            android:layout_height="35.0dp"
            android:id="@+id/textView17"
            android:background="#EE7600"
            android:textAlignment="center" />
        <CheckedTextView
            android:id="@+id/checkedTextView1" />
        <CheckedTextView
            android:id="@+id/checkedTextView2" />
        <CheckedTextView
            android:id="@+id/checkedTextView3" />
        <EditText
            android:inputType="number"
            android:id="@+id/editText1" />
        <EditText
            android:inputType="number"
            android:id="@+id/editText2" />
        <EditText
            android:inputType="numberDecimal"
            android:id="@+id/editText3" />
    </GridLayout>
    <LinearLayout
