                android:layout_marginLeft="10.0dp"
                android:layout_marginTop="16.0dp"
                android:id="@+id/huafeizhichu"
                android:textColor="#000000"
                android:layout_marginRight="0.0dp" />
            <ImageView
                android:src="@android:drawable/ic_menu_gallery"
                android:layout_width="30.0dp"
                android:layout_height="30.0dp"
                android:layout_marginLeft="16.0dp"
                android:layout_marginTop="16.0dp"
                android:id="@+id/star" />
        </GridLayout>
        <GridLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="62.0dp"
            android:id="@+id/gridLayout6"
            android:background="#FFF">
            <ImageView
                android:src="@android:drawable/ic_menu_gallery"
                android:id="@+id/image-shenghuoyongpin"
                android:layout_width="50.0dp"
                android:layout_height="50.0dp"
